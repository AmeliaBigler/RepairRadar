<div class="container">
    <div class="row">
        <div class="col">
            <h2>Create a New Ticket</h2>
        </div>
    </div>
</div>

<form class="form ticket-form">
  <div class="form-group">
    <label for="title">Title:</label>
    <input class="form-input" type="text" id="title"/>
  </div>
  <div class="form-group">
    <label for="car-make">Car Make:</label>
    <input class="form-input" type="text" id="carMake"/>
  </div>
  <div class="form-group">
    <label for="car-model">Car Model:</label>
    <input class="form-input" type="text" id="carModel"/>
  </div>
  <div class="form-group">
    <label for="car-model">Model Year:</label>
    <input class="form-input" type="text" id="modelYear"/>
  </div>
  <div class="form-group">
    <label for="issue">Issue:</label>
    <textarea class="form-control" id="issue" rows="3"></textarea>
  </div>
  <div class="form-group">
    <button class="btn btn-primary" type="submit">Sign Up</button>
  </div>
</form>

<script src="../js/newTicket.js"></script>